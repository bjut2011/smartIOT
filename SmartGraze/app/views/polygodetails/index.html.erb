<p id="notice"><%= notice %></p>

<h1>Listing Polygodetails</h1>

<table>
  <thead>
    <tr>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polygodetails.each do |polygodetail| %>
      <tr>
        <td><%= polygodetail.lat %></td>
        <td><%= polygodetail.lng %></td>
        <td><%= link_to 'Show', polygodetail %></td>
        <td><%= link_to 'Edit', edit_polygodetail_path(polygodetail) %></td>
        <td><%= link_to 'Destroy', polygodetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Polygodetail', new_polygodetail_path %>
