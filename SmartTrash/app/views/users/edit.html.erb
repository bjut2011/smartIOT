       function modify(id){
			if(confirm("确认要修改吗?")){
				 $.ajax({url:"/users/"+id, 
	                         type:"PUT",
	                         data:{'id':id,'name':$("#name").val(),'email':$("#email_id").val(),'mobile':$("#mobile_id").val(),'password':$("#password_id").val()},
