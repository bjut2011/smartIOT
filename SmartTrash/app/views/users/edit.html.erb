			var rq = $("#rq").val();
   			var h =  $("#h").val();
   			var m =  $("#m").val();
   			var s =  $("#s").val();
   			
   			s = Number(s)+1;
   			if(s < 10){
   			   s = "0" + s;
   			}
   			if(Number(s) == 60){
   				s = "0";
   				m = Number(m)+1;
   			}
   			if(m.length == 1){
   			    m = "0" + m;
   			}
