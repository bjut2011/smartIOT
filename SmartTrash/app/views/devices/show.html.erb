   				m = "0";
   				h = Number(h)+1;
   			}
   			if(h.length == 1){
   				h = "0" + h;
   			}
   			$("#rq").val(rq);
   			$("#h").val(h);
   			$("#m").val(m);
   			$("#s").val(s);
   			
   			var nowTime = rq+" "+h+":"+m+":"+s;
   			$("#nowDate").html(nowTime);
		}
    </script>
