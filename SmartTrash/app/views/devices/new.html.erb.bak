<div class="device-wrapper grid ">
        <div class="nav-avatar" xmlns="http://www.w3.org/1999/html">
            <img class="avatar-img j_avatar_img" src="http://upfiles.heclouds.com/123/imgList/2016/03/21/e84a0027f1b2d7569d5d7296f224e3e0.png">
            <h1>接入设备</h1>
            <em>Device</em>
            <div class="hr"></div>
        </div>
        <div class="content">
            <form class="standard form-horizontal j_device_add " data-parsley-validate="">
                <input type="hidden" name="device_id" id="device_id" value="">
                <div class="form-group type">
                    <label for="proj_id">所属项目：</label>
                    <select name="proj_id" id="proj_id" class="j_project_name">
                                <%= Project.all.each  do |pr| %>
                                                            <option value=<%= pr.id %> >
                                    <%= pr.name %>
                                </option>
                                  <% end %>
                                                    </select>
                                    </div>
                <div class="form-group">
                    <label for="device_name">设备名称：</label>
                    <input type="text" name="device_name" id="device_name" value="" required="" data-parsley-length="[1, 20]">
                </div>
                <div class="form-group">
                    <label for="device_mark">设备标签：</label>
                    <input type="text" name="device_mark" id="device_mark" placeholder="每个标签请用英文逗号隔开" value="">
                </div>
                <div class="form-group">
                    <label for="device_details">设备描述：</label>
                    <textarea name="device_details" id="device_details"></textarea>
                </div>
                <div class="form-group img-group">
                    <label for="device_img">添加图片：</label>
                    <div class="filed filed-200">
                        <img class="input-avatar j_avatar_img" src="http://upfiles.heclouds.com/123/imgList/2016/03/21/e84a0027f1b2d7569d5d7296f224e3e0.png">
                        <input type="hidden" name="device_img" id="device_img" value="http://upfiles.heclouds.com/123/imgList/2016/03/21/e84a0027f1b2d7569d5d7296f224e3e0.png">
                    </div>
                    <div class="img-select">
                        <div class="slimScrollDiv" style="position: relative; overflow: hidden; width: auto; height: 100%;"><div class="img-content" style="overflow: hidden; width: auto; height: 100%;"></div><div class="slimScrollBar" style="width: 7px; position: absolute; top: 0px; opacity: 0.4; display: block; border-radius: 7px; z-index: 99; right: 1px; background: rgb(0, 0, 0);"></div><div class="slimScrollRail" style="width: 7px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; opacity: 0.2; z-index: 90; right: 1px; background: rgb(51, 51, 51);"></div></div>
                    </div>
                    <a class="filed j_edit_head" href="#">修改默认</a>
                    <a class="filed j_upload_head" href="#">本地上传</a>
                </div>
                <div class="form-group">
                    <label for="device_protocol">接入协议：</label>
                                            <input type="hidden" name="device_protocol" id="device_protocol" value="HTTP">
                        <a class="hollow button limit j_device_protocol_http" href="#">HTTP协议</a>
                        <a class="secondary button limit j_device_protocol_edp" href="#">EDP协议</a>
                                    </div>
                <div class="form-group auth-group j_auth_group" style="display: none;">
                    <label for="device_auth">鉴权信息：</label>
                    <input type="hidden" name="device_auth" id="device_auth" value="">
                    <a class="button limit j_add_auth" href="#"><span class="icon-plus"></span>添加</a>
                    <div class="device-auth-dialog j_auth_dialog">
                        <div class="title">添加鉴权信息</div>
                        <div class="form-group">
                            <label for="param_name">参数名：</label>
                            <input type="text" name="param_name" id="param_name" value="">
                        </div>
                        <div class="form-group">
                            <label for="param_value">参数值：</label>
                            <input type="text" name="param_value" id="param_value" value="">
                        </div>
                        <div class="j_invalid_auth_message position-error"></div>
                        <div class="form-group">
                            <a class="button j_dialog_submit" href="#">提交</a>
                            <a class="secondary button j_dialog_cancel" href="#">取消</a>
                        </div>
                    </div>
                    <ul class="j_auth_ul">
                                            </ul>
                </div>
                <div class="form-group j_router_group" style="display: none;">
                    <label for="device_route">默认路由：</label>
                    <input type="text" name="device_route" id="device_route" value="" placeholder="默认路由值为已存在的设备ID">
                </div>
                <div class="form-group">
                    <label for="device_data">设备权限：</label>
                                        <input type="hidden" name="device_data" id="device_data" value="0">
                    <a class="secondary button limit j_device_data_private" href="#">私有数据</a>
                    <a class="hollow button limit j_device_data_open" href="#">公开数据</a>
                                    </div>
                <div class="form-group device-position-group">
                    <label for="device_position">设备位置：</label>
                    <input type="text" name="device_position" id="device_position" placeholder="通过输入然后选择或点击地图来确定坐标" value="" autocomplete="off">
